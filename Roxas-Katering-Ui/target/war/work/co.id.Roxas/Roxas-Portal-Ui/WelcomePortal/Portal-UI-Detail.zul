	<window  height="26.7%" width="15.4%" sclass="windowThree">
     <div style="background-color: #DBDBDB; width: 200%; height : 70%; text-algin : center; position : relative; left : -20px; top:-16px;">
     <hbox style="position :relative; left : 30px; top : 10px;">
     <vbox>
     <label id="hour" sclass="time" style="font-family: 'Tangerine', serif;
        font-size: 30px;"/>	
     <label value="Jam"/>
     </vbox>
     <label value=":" style="font-family: 'Tangerine', serif;
        font-size: 30px;"/>
     <vbox>
     <label id="minute" sclass="time" style="font-family: 'Tangerine', serif;
        font-size: 30px;"/>
     <label value="Menit"/>
     </vbox>
     <label value=":" style="font-family: 'Tangerine', serif;
        font-size: 30px;"/>
     <vbox>
     <label id="second" sclass="time" style="font-family: 'Tangerine', serif;
        font-size: 30px;"/>
     <label value="Detik"/>
     </vbox>
     </hbox>
     </div>
     <button iconSclass = "z-icon-bookmark positionSclass" label="edit event" height= "45px" width = "125%"
      style="position : relative; left : -17px; bottom : -2px;"/>	
	 <timer id="timer" delay="1000" repeats="true">
        <attribute name="onTimer">
        Date time = new Date();
        String h = new String(time.getHours()+"");
        String m = new String(time.getMinutes()+"");
        String s = new String(time.getSeconds()+"");
        if(h.length()==1)
        {
         hour.setValue("0"+time.getHours());
        }
        else{
        hour.setValue(time.getHours()+"");
        }
        if(m.length()==1)
        {
        minute.setValue("0"+time.getMinutes());
        }
        else{
        minute.setValue(time.getMinutes()+"");
        }
         if(s.length()==1)
        {
         second.setValue("0"+time.getSeconds());
        }
        else{
        second.setValue(time.getSeconds()+"");
        }
        </attribute>
    </timer>
	</window >
	
	
