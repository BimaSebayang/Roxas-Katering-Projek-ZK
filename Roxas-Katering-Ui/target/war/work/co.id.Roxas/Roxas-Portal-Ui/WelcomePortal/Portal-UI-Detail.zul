   <div sclass="buttonLogOut">
   <button iconSclass="z-icon-power-off" label="log out" sclass="powerOffButton" onClick="@command('logOut')">
        <attribute name="onMouseOver">
         self.setSclass("powerOffButtonOnMouse");
		</attribute>
		<attribute name="onMouseOut">
		 self.setSclass("powerOffButton");
		</attribute>   
   </button>
   <separator height="50px"/>
   <button iconSclass="z-icon-wrench" label="setting" sclass="settingButton">
    <attribute name="onMouseOver">
         self.setSclass("settingButtonOnMouse");
		</attribute>
		<attribute name="onMouseOut">
		 self.setSclass("settingButton");
		</attribute>   
   </button>
   <separator height="50px"/>
   <button iconSclass="z-icon-cogs" label="help"  sclass="helpButton">
    <attribute name="onMouseOver">
         self.setSclass("helpButtonOnMouse");
		</attribute>
		<attribute name="onMouseOut">
		 self.setSclass("helpButton");
		</attribute>   
   </button>
   </div>
	
	
